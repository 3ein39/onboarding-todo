    "@sentry/vue": "9.34.0",
    "@tailwindcss/vite": "4.1.6",
    "@tanstack/vue-query": "5.81.5",
    "@vueuse/core": "13.4.0",
    "@vueuse/router": "13.4.0",
    "@wisemen/vue-core-auth": "2.0.1",
    "@wisemen/vue-core-components": "1.9.0",
    "@wisemen/vue-core-modules": "2.3.2",
    "clsx": "2.1.1",
    "cva": "1.0.0-beta.1",
    "dayjs": "1.11.13",
    "formango": "3.1.0",
    "i18n-iso-countries": "7.14.0",
    "jwt-encode": "1.0.1",
    "libphonenumber-js": "1.12.9",
    "motion-v": "1.3.1",
    "nats.ws": "^1.30.3",
    "neverthrow": "^8.2.0",
    "pinia": "3.0.3",
    "reka-ui": "2.3.1",
    "tailwind-merge": "3.3.1",
    "vue": "3.5.17",
    "vue-highlight-words": "3.0.1",
    "vue-i18n": "12.0.0-alpha.1",
    "vue-router": "4.5.1",
