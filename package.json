    "@playwright/test": "1.52.0",
    "@tailwindcss/vite": "4.1.11",
    "@tanstack/vue-query-devtools": "5.81.5",
    "@tsconfig/node22": "22.0.2",
    "@types/jsdom": "21.1.7",
    "@types/jwt-encode": "1.0.3",
    "@types/node": "24.0.8",
    "@vitejs/plugin-vue": "6.0.0",
    "@vue/language-server": "2.2.10",
