{
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "vite",
    "development": "vue-tsc --noEmit && vite build --mode development",
    "staging": "vue-tsc --noEmit && vite build --mode staging",
    "production": "vue-tsc --noEmit && vite build --mode production",
    "build": "vite build",
    "preview": "import-meta-env -x .env.test && vite preview",
    "openapi-ts": "openapi-ts -f openapi.config.ts",
    "lint:eslint": "eslint . --fix --cache",
    "lint:oxlint": "oxlint . --fix",
    "oxlint:migrate": "pnpx @oxlint/migrate eslint.config.js --with-nursery --merge",
    "lint": "pnpm lint:oxlint && pnpm lint:eslint",
    "test:unit": "vitest --run",
    "test:e2e": "npx playwright test",
    "test:coverage": "nyc pnpm test:e2e && nyc report --reporter=html",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky",
    "precommit": "pnpm lint && pnpm type-check && pnpm test:unit && pnpm build && pnpm test:coverage"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.2.6",
